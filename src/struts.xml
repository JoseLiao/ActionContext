<?xml version="1.0" encoding="UTF-8"?>
<struts>
    <contants name="conststruts.devMode" value="true"></contants>
    <package name="joseph" extends="struts-default">
        <action name="login" class="joseph.action.LoginAction">
            <result name="error">/WEB-INF/content/error.jsp</result>
            <result>/WEB-INF/content/welcome.jsp</result>
        </action>
		<action name="*">
		    <result>/WEB-INF/content/{1}.jsp</result>
		</action>        
        
    </package>
    
</struts>